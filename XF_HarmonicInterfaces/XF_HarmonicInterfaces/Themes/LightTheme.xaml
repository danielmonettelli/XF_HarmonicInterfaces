<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary
    x:Class="XF_HarmonicInterfaces.Themes.LightTheme"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:d="http://xamarin.com/schemas/2014/forms/design"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Source="/Styles/General.xaml"
    mc:Ignorable="d">

    <!--#region Color Range-->
    <Color x:Key="colPrim">#E68342</Color>
    <Color x:Key="colSec">#222B45</Color>
    <Color x:Key="colTer">#38415C</Color>
    <Color x:Key="colQua">#222B45</Color>
    <Color x:Key="colQui">#393C50</Color>
    <Color x:Key="colSex">#9D9DAB</Color>
    <Color x:Key="colSep">#686C83</Color>
    <Color x:Key="colOct">#38415C</Color>
    <Color x:Key="colNon">#393C50</Color>
    <Color x:Key="colTen">#38415C</Color>
    <!--#endregion-->

    <!--#region Backgroundcolors-->
    <Color x:Key="colStartBG1">#F8F8F8</Color>
    <Color x:Key="colEndBG1">#F8F8F8</Color>
    <Color x:Key="colStartBG2">#F8F8F8</Color>
    <Color x:Key="colEndBG2">#F8F8F8</Color>
    <!--#endregion-->

